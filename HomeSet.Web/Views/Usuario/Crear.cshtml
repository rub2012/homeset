@model UsuarioDto
@{
    Layout = null;
    ViewData["Title"] = "Nuevo Usuario";
}

<script src="~/lib/jquery.validate.min.js"></script>
<script src="~/lib/jquery.validate.unobtrusive.js"></script>
<script src="~/lib/knockout-latest.js"></script>
<script src="~/js/usuarioCrearModificar.js" asp-append-version="true"></script>
<script src="~/js/rol.js" asp-append-version="true"></script>



<form asp-controller="Usuario" asp-action="Crear"
      data-ajax="true"
      data-ajax-method="POST">
    @await Html.PartialAsync("_CrearModificar", Model)

</form>