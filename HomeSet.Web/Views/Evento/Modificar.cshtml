@model EventoDto
@{
    Layout = null;
    ViewData["Title"] = "Modificar Evento";
}
@Html.Hidden("Modificando", true)
<link rel="stylesheet" href="~/lib/jquery.datetimepicker.min.css" />
<script src="~/lib/jquery.validate.min.js"></script>
<script src="~/lib/jquery.validate.unobtrusive.js"></script>
<script src="~/lib/messages_es_AR.min.js"></script>
<script src="~/lib/jquery.datetimepicker.full.js"></script>
<script src="~/js/validacionesCustom.js" asp-append-version="true"></script>
<script src="~/js/eventosCrearModificar.js" asp-append-version="true"></script>
<form asp-controller="Evento" asp-action="Modificar"
      data-ajax="true"
      data-ajax-method="POST">
    @await Html.PartialAsync("_CrearModificar", Model)

</form>